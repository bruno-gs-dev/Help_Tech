{
    "version": 2,
    "outputDirectory": ".",
    "rewrites": [
        {
            "source": "/login",
            "destination": "/login/index.html"
        },
        {
            "source": "/register",
            "destination": "/register/index.html"
        },
        {
            "source": "/forgot-password",
            "destination": "/forgot-password/index.html"
        },
        {
            "source": "/dashboard",
            "destination": "/dashboard/index.html"
        },
        {
            "source": "/dashboard/admin",
            "destination": "/dashboard/admin/index.html"
        },
        {
            "source": "/dashboard/profile",
            "destination": "/dashboard/profile/index.html"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ]
}